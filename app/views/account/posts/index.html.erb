<div class="col-md-12">
  <table class="table">
    <thead>
      <th>#</th>
      <th>Content</th>
      <th>Last Updated</th>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
        <td>#</td>
        <td><%= post.content %></td>
        <td><%= post.updated_at %></td>
        <td>
        <%= link_to("Edit", edit_group_post_path(post.group, post),class: "btn btn-default") %>
        <%= link_to("Delete", group_post_path(post.group,post), class: "btn btn-default", method: :delete) %>
        </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
